spring.application.name=cuenta-bancaria
spring.datasource.schema = ${paf_sqlserver_schema}
server.port=8082

# Conexion [SQLServer] PagosAF
spring.datasource.paf.driverClassName=${paf_sqlserver_driverclassname}
spring.datasource.paf.url=${paf_spring_host_url}
spring.datasource.paf.username=${paf_sqlserver_username}
spring.datasource.paf.password=${paf_sqlserver_password}
spring.mvc.pathmatch.matching-strategy = ANT_PATH_MATCHER
spring.jpa.properties.hibernate.dialect=${paf_sqlserver_dialect}

# Conexion [Oracle] GenesysD
spring.datasource.driverClassName=${paf_oracle_driverclassname}
spring.datasource.url=${paf_oracle_url}
spring.datasource.username=${paf_oracle_username}
spring.datasource.password=${paf_oracle_password}

